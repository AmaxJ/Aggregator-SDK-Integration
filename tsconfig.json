{
  "compilerOptions": {
    "target": "esnext",
    "module": "ES2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "emitDeclarationOnly": false,
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts","router/**/*.ts", "fixedNumber/**/*.ts", "pyth/**/*.ts", "typechain/**/*.ts", ".eslintrc.cjs"]
}
